{
  "name": "chinese-learning-app",
  "description": "Chinese Learning Application - React Version",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "keywords": [
    "react",
    "typescript",
    "antd",
    "tailwindcss",
    "chinese-learning",
    "vite"
  ],
  "scripts": {
    "start": "vite dev",
    "build": "tsc && cross-env NODE_ENV=production vite build",
    "preview": "vite preview",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "npm run eslint -- src",
    "lint:fix": "npm run eslint -- --fix src",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "prettify": "prettier --write",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint -- --fix"
    ],
    "*.{css,scss}": [
      "npm run lint:css"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@iconify/react": "^5.0.2",
    "@reduxjs/toolkit": "1.9.7",
    "@types/node": "^22.10.2",
    "antd": "^5.20.2",
    "axios": "^1.11.0",
    "framer-motion": "^12.23.6",
    "js-cookie": "3.0.5",
    "lucide-react": "^0.263.1",
    "qs": "6.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "7.2.9",
    "react-router-dom": "^6.14.2",
    "reapop": "4.3.0",
    "redux-injectors": "2.1.0",
    "redux-saga": "1.3.0",
    "redux-saga-routines": "3.2.3",
    "regenerator-runtime": "0.14.1",
    "styled-components": "^5.3.11",
    "vite-plugin-environment": "1.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/qs": "6.14.0",
    "@types/react": "^18.2.40",
    "@types/react-dom": "^18.2.15",
    "@types/redux-saga-routines": "3.1.8",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "babel-plugin-macros": "^3.1.0",
    "cross-env": "^7.0.3",
    "devmoji": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-branch-is": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.45",
    "prettier": "^3.4.0",
    "stylelint": "^16.0.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "5.1.6",
    "vite": "^5.4.19",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-checker": "^0.8.0",
    "vite-tsconfig-paths": "5.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
